mainclass Fibonacci {
  public static void main() {
    int first, second, i, tmp;
    first = 0;
    second = 1;
    i = 0;
    while (i < 10) {
      i = i + 1;
      tmp = first + second;
      println(tmp);
      first = second;
      second = tmp;
    }
  }
}



